name:                phil2
version:             0.1.0.0
-- synopsis:
-- description:
license:             BSD3
license-file:        LICENSE
author:              Isaac Elliott
maintainer:          isaace71295@gmail.com
-- copyright:
-- category:
build-type:          Simple
extra-source-files:  ChangeLog.md
cabal-version:       >=1.10

library
  exposed-modules:     Phil.Core
                     , Phil.Builtins
                     , Phil.Eval
                     , Phil.Parser
                     , Phil.Typecheck
  build-depends:       base >=4.9 && <4.11
                     , lens >=4.15 && <4.16
                     , containers >=0.5 && <0.6
                     , mtl >=2.2 && <2.3
                     , deriving-compat >=0.3 && <0.4
                     , trifecta >=1.7 && <1.8
                     , parsers >=0.12 && <0.13
                     , bytestring >=0.10 && <0.11
                     , unification
  hs-source-dirs:      src
  ghc-options:         -Wall -Wincomplete-record-updates
                       -Wunused-imports -Werror
  default-language:    Haskell2010
